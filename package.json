{
  "name": "gt1191-starterkit",
  "private": true,
  "version": "1.10.1",
  "browserslist": "> 2%, last 2 versions, not dead",
  "scripts": {
    "dev": "parcel \"src/**/*.html\" --open",
    "clean": "shx rm -rf ./.parcel-cache ./dist",
    "build": "yarn run clean && parcel build src/*.html",
    "deliver": "shx rm -rf ./node_modules ./dist ./.parcel-cache yarn.lock"
  },
  "target": {
    "default": {
      "distDir": "dist",
      "publicUrl": "/"
    }
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "proseWrap": "preserve",
    "singleAttributePerLine": false,
    "overrides": [
      {
        "files": [
          ".",
          "**/*.html"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "@fontsource-variable/rubik": "^5.0.22"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.12.0",
    "autoprefixer": "^10.4.19",
    "parcel": "^2.12.0",
    "postcss": "^8.4.38",
    "posthtml-expressions": "^1.11.3",
    "posthtml-include": "^2.0.1",
    "sharp": "^0.31.3",
    "shx": "^0.3.4"
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true,
      "customMedia": true
    }
  }
}
