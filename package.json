{
  "name": "gt1191-starterkit",
  "private": true,
  "version": "1.7.2",
  "browserslist": "> 2%, last 2 versions, not dead",
  "scripts": {
    "dev": "parcel \"src/**/*.html\" --open",
    "clean": "shx rm -rf ./.parcel-cache ./dist",
    "build": "yarn run clean && parcel build src/*.html",
    "deliver": "shx rm -rf ./node_modules ./dist ./.parcel-cache yarn.lock"
  },
  "target": {
    "default": {
      "distDir": "dist",
      "publicUrl": "/"
    }
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "proseWrap": "preserve",
    "singleAttributePerLine": false,
    "overrides": [
      {
        "files": [
          ".",
          "**/*.html"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.8.3",
    "@parcel/transformer-sass": "2.8.3",
    "autoprefixer": "^10.4.14",
    "parcel": "latest",
    "postcss": "^8.4.23",
    "posthtml-include": "^1.7.4",
    "sass": "^1.62.1",
    "sharp": "^0.31.1",
    "shx": "^0.3.4"
  }
}
