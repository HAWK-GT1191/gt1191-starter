{
  "name": "gt1191-starterkit",
  "private": true,
  "version": "1.0.0",
  "browserslist": "> 2%, last 2 versions, not dead",
  "target": {
    "distDir": "./dist"
  },
  "scripts": {
    "dev": "parcel src/*.html --open",
    "clean": "rm -rf ./.parcel-cache ./dist",
    "build": "yarn run clean && parcel build src/*.html"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "proseWrap": "preserve",
    "singleQuote": true
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.5.0",
    "@parcel/transformer-sass": "2.5.0",
    "parcel": "latest",
    "posthtml-expressions": "^1.9.0",
    "posthtml-include": "^1.7.3",
    "sass": "^1.51.0",
    "sharp": "^0.29.1"
  },
  "dependencies": {
    "autoprefixer": "^10.4.6",
    "postcss": "^8.4.13"
  }
}
