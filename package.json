{
  "name": "gt1191-starterkit",
  "private": true,
  "version": "1.0.0",
  "browserslist": "> 2%, last 2 versions, not dead",
  "target": {
    "distDir": "./dist"
  },
  "scripts": {
    "dev": "parcel './src/*.html' --open",
    "clean": "shx rm -rf ./.parcel-cache ./dist",
    "build": "yarn run clean && parcel build src/*.html",
    "deliver": "shx rm -rf ./node_modules"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "proseWrap": "preserve",
    "singleQuote": true
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "parcel": "2.7.0",
    "posthtml-expressions": "^1.10.0",
    "posthtml-include": "^1.7.4",
    "sass": "^1.56.0",
    "sharp": "^0.29.1",
    "shx": "^0.3.4"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.18"
  }
}
